{
  "name": "Sky Pix",
  "description": "Shows a sky picture every time you open a new tab",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "cloud.png"
    },
  "chrome_url_overrides": {
    "newtab": "demo.html"
    },
  "permissions": [
    "tabs"
    ],
    "background": {
    "scripts": ["background.js"],
    "persistent": false
    }
}
